  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint --cache .",
    "typecheck": "tsc -p scripts --noEmit && tsc -p playground --noEmit",
    "test": "run-s test-serve test-build",
    "test-serve": "vitest run -c vitest.config.e2e.ts",
    "test-build": "KOLIBRY_TEST_BUILD=1 vitest run -c vitest.config.e2e.ts",
    "test-build-without-plugin-commonjs": "VITE_TEST_WITHOUT_PLUGIN_COMMONJS=1 pnpm test-build",
    "debug-serve": "KOLIBRY_DEBUG_SERVE=1 vitest run -c vitest.config.e2e.ts",
    "debug-build": "KOLIBRY_TEST_BUILD=1 VITE_PRESERVE_BUILD_ARTIFACTS=1 vitest run -c vitest.config.e2e.ts",
    "build": "pnpm -r --filter='./packages/*' run build",
    "dev": "pnpm -r --parallel --filter='./packages/*' run dev",
    "release": "dynot scripts/release.ts",
    "ci-publish": "dynot scripts/publishCI.ts"
  },
